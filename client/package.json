    "ts-migrate": "^0.1.35",
    "url-join": "^5.0.0",
    "yup": "^1.1.1"
  },
  "scripts": {
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:start": "cd dist && PORT=3002 npx serve",
    "test": "jest --env=jsdom",
    "start": "webpack serve --open --mode development",
    "start:live": "webpack serve --open --mode development --live-reload --hot",
    "coverage": "jest --coverage --coverageReporters=cobertura"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/setup/browserMocks.tsx"
    ],
    "testEnvironment": "jsdom",
    "globals": {
      "IS_REACT_ACT_ENVIRONMENT": true
    },
    "collectCoverage": true,
    "coverageReporters": [
      "cobertura",
      "html",
      "text"
    ],
    "moduleNameMapper": {
      "\\.(jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/mocks/fileMock.ts",
      "\\.(css|less|scss)$": "<rootDir>/src/mocks/styleMock.ts"
    },
    "reporters": [
      "default",
      "jest-junit"
    ]
  },
  "jest-junit": {
    "output": "coverage/junit/junit.xml",
    "usePathForSuiteName": "true"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-jsx": "^7.23.3",
    "@types/debounce": "^1.2.4",
    "@types/react-datepicker": "^4.19.6",
    "@types/react-dom": "^18.2.25",
    "@types/react-query": "^1.2.9",
    "@types/react-table": "^7.7.20",
    "css-loader": "^7.1.2",
    "html-webpack-plugin": "^5.5.1",
    "http-proxy-middleware": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.3",
    "sass": "^1.71.1",
    "sass-loader": "^14.1.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "svg-url-loader": "^8.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.2",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "overrides": {
    "react-scripts": {
      "typescript": "^5"
    },
    "nth-check": "2.1.1"
  }
}
